package program;

public class ReverseString {
    public static void main(String[] args) {
        String s1 = "i.like.this.program.very.much";
        String[] str = s1.split("\\.");

        for(int i = str.length-1; i >= 0 ; i--){
                System.out.print(str[i]);
                if(i != 0){
                    System.out.print(".");
                }
        }
    }
}
